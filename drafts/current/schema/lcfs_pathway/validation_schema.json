{
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://github.com/carbondirect/BOOST/schemas/lcfs-pathway",
    "title": "LCFS Pathway",
    "description": "CARB-certified fuel pathway for LCFS compliance with carbon intensity and regulatory attributes",
    "type": "object",
    "required": [
      "@context",
      "@type",
      "@id",
      "pathwayId",
      "pathwayType",
      "feedstockCategory",
      "fuelProduct",
      "facilityLocation",
      "carbonIntensity",
      "energyEconomyRatio",
      "certificationDate",
      "verificationStatus",
      "caGreetVersion"
    ],
    "properties": {
      "@context": {
        "type": "object",
        "description": "JSON-LD context for semantic web compatibility"
      },
      "@type": {
        "type": "string",
        "const": "LCFSPathway"
      },
      "@id": {
        "type": "string",
        "format": "uri",
        "description": "Unique URI identifier for JSON-LD"
      },
      "pathwayId": {
        "type": "string",
        "description": "CARB-assigned pathway identifier",
        "pattern": "^CA-[A-Z]{2,4}-[0-9]{4}-[A-Z]{3,4}-[0-9]{3}$"
      },
      "pathwayType": {
        "type": "string",
        "enum": [
          "Lookup_Table",
          "Tier_1",
          "Tier_2"
        ],
        "description": "CARB pathway certification tier"
      },
      "feedstockCategory": {
        "type": "string",
        "enum": [
          "corn",
          "sugarcane",
          "forest_residue",
          "agricultural_residue",
          "logging_and_mill_residue",
          "grass_residue_waste",
          "forest_harvest_residue",
          "urban_wood_waste",
          "dedicated_energy_crops",
          "algae",
          "used_cooking_oil",
          "animal_fats",
          "other_waste_oils"
        ],
        "description": "Primary feedstock type for pathway"
      },
      "fuelProduct": {
        "type": "string",
        "enum": [
          "ethanol",
          "renewable_diesel",
          "biodiesel",
          "sustainable_aviation_fuel",
          "renewable_gasoline",
          "hydrogen",
          "renewable_natural_gas",
          "electricity"
        ],
        "description": "Final fuel product produced"
      },
      "facilityLocation": {
        "type": "string",
        "description": "Production facility location (city, state or geographic region)"
      },
      "carbonIntensity": {
        "type": "number",
        "minimum": 0,
        "maximum": 200,
        "description": "Certified carbon intensity in gCO2e/MJ"
      },
      "energyEconomyRatio": {
        "type": "number",
        "minimum": 0.5,
        "maximum": 3.0,
        "description": "Energy economy ratio multiplier for credit calculation"
      },
      "certificationDate": {
        "type": "string",
        "format": "date",
        "description": "CARB pathway certification date"
      },
      "expirationDate": {
        "type": "string",
        "format": "date",
        "description": "Pathway certification expiration date"
      },
      "verificationStatus": {
        "type": "string",
        "enum": [
          "active",
          "suspended",
          "expired",
          "pending_renewal"
        ],
        "description": "Current CARB verification status"
      },
      "caGreetVersion": {
        "type": "string",
        "description": "CA-GREET model version used for pathway certification",
        "pattern": "^[0-9]+\\.[0-9]+$"
      },
      "facilityCapacity": {
        "type": "number",
        "minimum": 0,
        "description": "Annual production capacity in gallons"
      },
      "processDescription": {
        "type": "string",
        "description": "Brief description of production process"
      },
      "geographicScope": {
        "type": "string",
        "enum": [
          "California",
          "Western_US",
          "National",
          "International"
        ],
        "description": "Geographic applicability of pathway"
      },
      "lastUpdated": {
        "type": "string",
        "format": "date-time",
        "description": "Timestamp of most recent pathway data update"
      }
    },
    "additionalProperties": false
  },
  "boost_metadata": {
    "entity": {
      "name": "LCFSPathway",
      "primaryKey": "pathwayId",
      "area": "compliance_reporting",
      "description": "LCFS pathway definitions for fuel lifecycle tracking"
    },
    "relationships": []
  }
}